marks = int(input("Enter your marks: "))

if marks >= 40:
    print("Pass")
    if marks >= 75:
        print("Grade: Distinction")
    elif marks >= 60:
        print("Grade: First Class")
    elif marks >= 50:
        print("Grade: Second Class")
    else:
        print("Grade: Pass Class")
else:
    print("Fail")
